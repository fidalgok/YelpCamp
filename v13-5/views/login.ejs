<% include partials/header %>

	<h1 class="text-center">Login</h1>
	<div class="auth-form">
		<form method="POST" action="/login" >
			<div class="form-group">
				<label for="inputUsername">
					Username
				</label>
				<input class="form-control" type="text" name="username" placeholder="username" id="inputUsername">
			</div>
			<div class="form-group">
				<label for="inputPassword">
					Password
				</label>
				<input class="form-control" type="password" name="password" placeholder="Password" id="inputPassword">
			</div>
			
			<div class="form-group">
				<input type="submit" value="submit" class="btn btn-default btn-block">
			</div>
		</form>
	</div>

<% include partials/footer %>